bool reverse(char* s,int left,int right)
{
    while(left<right)
    {
        if(s[left]!=s[right])
        {
            return false;
        }
        left++;
        right--;
    }
    return true;
}
int countSubstrings(char* s) {
    int n=0;
    for(int i=0;i<strlen(s);i++)
    {
        for(int j=i;j<strlen(s);j++)
        {
            if(reverse(s,i,j))
            {
                n++;
            }
        }
    }
    return n;
}