char* restoreString(char* s, int* indices, int indicesSize) {
    char* ret=(char*)malloc(sizeof(char)*(indicesSize+1));
    int n=0;
    int k=indicesSize;
    while(k--)
    {
      int flag=0;
      for(int i=0;i<indicesSize;i++)
      {
        if(indices[i]==n)
        {
          flag=i;
          break;
        }
      }
      ret[n++]=s[flag];
    }
    ret[n]='\0';
    return ret;
}