int* numberOfLines(int* widths, int widthsSize, char* s, int* returnSize) {
    int* ret=(int*)malloc(sizeof(int)*2);
    int n=0;
    int flag=1;
    for(int i=0;i<strlen(s);i++)
    {
        int m=s[i]-'a';
        n+=widths[m];
        if(n>100)
        {
            flag++;
            n=widths[m];
        }
    }
    ret[0]=flag;
    ret[1]=n;
    *returnSize=2;
    return ret;
}