int maxDepth(char* s) {
    int n=strlen(s);
    char stack[100];
    int m=0;
    int max=0;
    for(int i=0;i<n;i++)
    {
        if(s[i]=='(')
        {
            stack[m++]=')';
            if(m>max)
            {
                max=m;
            }
        }
        if(s[i]==')')
        {
            m--;
        }
    }
    return max;
}